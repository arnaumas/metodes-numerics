set terminal epslatex size 10cm,6cm color colortext header '\sffamily' '\small'
set output '32_eq.tex'

set format '\num{%g}'
set border linewidth 1.5

set xtics scale 0.75
set yrange [0:2]
set ytics scale 0.75

set style line 1 linecolor rgb "orange-red"
set style line 2 linecolor rgb "blue"

plot '32_avaluats.dat' using 1:2 notitle with lines linestyle 1, '32_eq_interpolats.dat' using 1:2 notitle with lines linestyle 2
